  <% review.comments.each do |comment| %>
  <p>
    <%= image_tag comment.member.get_profile_image(100,100) %>
  </p>
    <%= comment.created_at.strftime('%Y/%m/%d') %>
    <%= comment.member.name %><br>
    <%= comment.comment_cont %>
  <p>
    <% if comment.member == current_member or admin_signed_in? %>
      <%= link_to "削除", review_comment_path(comment.review, comment), method: :delete, remote: true %>
    <% end %>
  <% end %>
  </p>
