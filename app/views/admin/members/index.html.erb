<div class= "container">
  <div class= "row">
    <div class= "col-10 mx-auto">
     <% @members.each do |member| %>
         <table class="table table-hover table-inverse" >
           <thead>
             <tr>
               <th>登録日時</th>
               <th>ユーザ名</th>
               <th>登録ステータス</th>
             </tr>
           </thead>
           <tbody>
             <tr>
               <td><%= member.created_at.strftime('%Y/%m/%d') %></td>
               <td><%= link_to member.name, admin_member_path(member.id) %></td>

                 <td><% if member.is_deleted == false %>
                  有効
                 <% else %>
                  退会</td>
                 <% end %>
               </td>
             </tr>
           </tbody>
         </table>
     <% end %>
    </div>
  </div>
</div>